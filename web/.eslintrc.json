{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "prettier/standard",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-expressions": "off",
        "react/prop-types": "off",
        "space-before-function-paren": "off",
        "no-explicit-any": "off",
        "camelcase": "off"
    }
}
